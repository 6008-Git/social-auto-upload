{
  "name": "AutoUploadVideo",
  "private": true,
  "version": "0.1.0",
  "author": "likang",
  "description": "自动上传视频系统",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:serve": "electron ./electron/main.cjs",
    "electron:dev": "node scripts/electron-dev.cjs",
    "electron:build": "npm run build && electron-builder"
  },
  "build": {
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "asar": false,
    "appId": "com.xinjiangUniversity.iams",
    "productName": "自动上传视频系统",
    "directories": {
      "output": "build-ele"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": {
        "target": "nsis",
        "arch": ["x64"]
      }
    },
    "npmRebuild": false,
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.txt",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "自动上传视频系统",
      "language": "2052",
      "perMachine": false,
      "runAfterFinish": true
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.9.0",
    "element-plus": "^2.9.11",
    "pinia": "^3.0.2",
    "sass": "^1.89.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "electron": "^35.1.2",
    "vite": "^6.3.5"
  }
}